		"twig/twig": "2.12.5"
	},
	"require-dev": {
		"behat/behat": "3.5.0",
		"facebook/webdriver": "1.7.1",
		"phpmd/phpmd": "2.7.0",
		"phpstan/phpstan": "0.11.12",
